#include<iostream>
using namespace std;
class dist 
{
public:
int fe;
int in;
int ans;
public:
void getdist()
{
 cout<<"enter the feet ";
 cin>>fe;
 cout<<"enter the inches ";
 cin>>in;
}
void displaydist()
{
 cout<<"inches=";
 cout<<in;
 cout<<"feet=";
 cout<<fe;
}
void add()
{
 ans=fe+(in/12);
 cout<<ans;
}
};
 int main()
{
 class dist d;
 d.getdist();
 d.displaydist();
 d.add();
}
